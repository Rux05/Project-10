{
  "devDependencies": {
    "cypress": "^13.11.0",
    "cypress-multi-reporters": "^1.6.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "chai": "^5.1.1"
  },
  "scripts": {
    "cypress:run": "cypress run",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/results/results.json",
    "generate:report": "marge cypress/results/results.json -f report -o cypress/reports/mochawesome"
  }
}
